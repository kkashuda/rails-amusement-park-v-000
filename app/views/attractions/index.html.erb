<% if !current_user.admin %> 
  <ul>
  <% @attractions.each do |attraction| %> 
    <li> <%= link_to "Go on #{attraction.name}", attraction_path(attraction) %>  </li>
  <% end %> 
  </ul> 
<% else %>   
  <% @attractions.each do |attraction| %>
    <%= link_to "Show #{attraction.name}", attraction_path(attraction) %> </br> 
  <% end %> 
  </br> <button> <%= link_to "New Attraction", new_attraction_path %> </button> 
<% end %> 